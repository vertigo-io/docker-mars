<Context debug="0" reloadable="false" allowCasualMultipartParsing="true">
        <Resource name="jdbc/DataSource"
                auth="SERVLET"
                type="javax.sql.DataSource"
                driverClassName="org.postgresql.Driver"
                url="jdbc:postgresql://postgresql:5432/mars"
                username="mars"
                password="mars" />

        <Parameter name="devMode" value="false" override="false"/>
        <Parameter name="boot.activeFlags" value="klee" override="false"/>
        <Parameter name="boot.apiKeysUrl" value="/usr/local/tomcat/marsconf/marsApiKeys.properties" override="false"/>
        <Parameter name="redisHost" value="redis" override="false"/>
        <Parameter name="analyticsHost" value="analytics" override="false"/>
        <Parameter name="influxdbHost" value="http://influxdb:8086" override="false"/>
        <Parameter name="esHost" value="elasticsearch:9300" override="false"/>
        <Parameter name="ledgerHost" value="http://localhost:8545" override="false"/>
        <Parameter name="mosquittoHost" value="tcp://mosquitto:1883" override="false"/>
        <Parameter name="orchestra.root.directory" value="/home/tomcat/marsOrchestra/" override="false"/>
        <Parameter name="suppliersImportLimit" value="3000000" override="false"/>
		<Parameter name="chatbotUrl" value="//localhost:5005/webhooks/rest/webhook" override="false"/>
        <Parameter name="apiManagementUrl" value="https://store.api.mars.vertigo.io/store/" override="false"/>
</Context>
